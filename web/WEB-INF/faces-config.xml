<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Lang/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>instructor</from-outcome>
            <to-view-id>/instructor/programas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>aprendiz</from-outcome>
            <to-view-id>/aprendiz/pruebaasistencia.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/Views/programa/List.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/instructor/programas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ins1</from-outcome>
            <to-view-id>/instructor/fichas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/instructor/fichas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ins2</from-outcome>
            <to-view-id>/instructor/eventos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/programas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ad1</from-outcome>
            <to-view-id>/admin/fichas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/fichas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ad2</from-outcome>
            <to-view-id>/admin/eventos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/aprendices.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ad4</from-outcome>
            <to-view-id>/admin/asistencias.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/eventos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ad3</from-outcome>
            <to-view-id>/admin/aprendices.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/instructor/eventos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ins3</from-outcome>
            <to-view-id>/instructor/asistencias.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
